  file = {IEEE Xplore Full Text PDF:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/3K7PT7KR/Fernandez and Williams - 2010 - Closed-Form Expression for the Poisson-Binomial Pr.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/5TG4HKEU/abs_all.html:}
