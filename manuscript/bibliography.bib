  keywords = {Combinatorial optimization,Optimal search,Production recipes,Search distance,Technology landscape},
  file = {ScienceDirect Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/8AFJ6BZD/S0167268100001141.html:}
