  langid = {english},
  keywords = {90B15,D85,Information Storage and Retrieval,Interdisciplinary Studies,Jeff_To_Read,Library Science,network analysis,O32,O33,Patent citation,Printed electronics,Technological convergence},
  file = {Kim_Cho_Kim_2013.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/5UTB6RC2/Kim_Cho_Kim_2013.pdf:application/pdf;Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/3E6RTFD4/s11192-013-1104-7.html:text/html}
