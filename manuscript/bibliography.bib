  pages = {415--426},
  keywords = {Complex networks,Education (general),evolution,Evolutionary Biology,Evolvability,Jeff_To_Read,Learning & Instruction,Sociology of Education,Spandrels,Teaching and Teacher Education,Technology_Development,Tinkering,To_Read},
  file = {Solé_Valverde_Rodriguez-Caso_2011.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CHED449Z/Solé_Valverde_Rodriguez-Caso_2011.pdf:application/pdf;Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/V6RJ8KVG/s12052-011-0346-1.html:}
