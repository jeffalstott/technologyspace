  date = {2007-08},
  pages = {318--339},
  keywords = {Extreme-value statistics,Heavy tails,Patent citations,Returns to invention,Skewed distributions},
  file = {ScienceDirect Full Text PDF:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/FA4DNFKI/Silverberg and Verspagen - 2007 - The size distribution of innovations revisited An.pdf:application/pdf;ScienceDirect Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/7WWDSA4H/S030440760600217X.html:text/html}
