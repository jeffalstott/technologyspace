  date = {2014-07-01},
  pages = {623--652},
  keywords = {030,031,Economic Growth,Economic Theory,Entrepreneurship,Microeconomics,Patent citation network,patent data,R & D/Technology Policy,Technology dynamics,Telecommunication manufacturing industry},
  file = {Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/IBCAFV49/s00191-014-0349-5.html:;Martinelli_Nomaler_2014.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/KBE72FM3/Martinelli_Nomaler_2014.pdf:application/pdf}
