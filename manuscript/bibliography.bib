  pages = {833--838},
  langid = {english},
  keywords = {brain connectivity,connectome,graph theory,network theory},
  file = {Goñi_et_al_2014.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/WNEWVJC7/Goñi_et_al_2014.pdf:application/pdf;Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/HCK2R9Z9/833.html:text/html},
