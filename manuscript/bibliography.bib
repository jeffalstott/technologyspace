  shortjournal = {Science},
  author = {Chen, Fei and Tillberg, Paul W. and Boyden, Edward S.},
  urldate = {2015-02-16},
  date = {2015-01-30},
  pages = {543--548},
  file = {Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/H245HBZQ/543.html:;Chen_Tillberg_Boyden_2015.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/WJF6CIC7/Chen_Tillberg_Boyden_2015.pdf:application/pdf},
