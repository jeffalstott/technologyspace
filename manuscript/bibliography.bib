  pages = {1--15},
  keywords = {Bibliometrics,Characterization and Evaluation of Materials,Inorganic Chemistry,Jeff_To_Read,Nanotechnology,Optics; Optoelectronics; Plasmonics and Optical Devices,Patent Analysis,Physical Chemistry,Printed electronics,Technology life cycle,Technology network},
  file = {Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CM3EER2P/s11051-014-2471-6.html:;Yoon_et_al_2014.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/F8IDNM7X/Yoon_et_al_2014.pdf:application/pdf}
