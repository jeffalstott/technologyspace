  file = {Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/V6RJ8KVG/s12052-011-0346-1.html:text/html;Solé_Valverde_Rodriguez-Caso_2011.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CHED449Z/Solé_Valverde_Rodriguez-Caso_2011.pdf:application/pdf}
