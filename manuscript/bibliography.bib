  file = {ScienceDirect Full Text PDF:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CJ8PUV4B/Shi et al. - 2015 - Weaving the fabric of science Dynamic network mod.pdf:application/pdf;ScienceDirect Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CMQ9G6FE/S0378873315000118.html:}
}

@article{Sgrignoli2015,
