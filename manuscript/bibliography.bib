@article{Traag2015,
  title = {Faster unfolding of communities: speeding up the Louvain algorithm},
  url = {http://arxiv.org/abs/1503.01322},
  shorttitle = {Faster unfolding of communities},
  abstract = {Many complex networks exhibit a modular structure of densely connected groups of nodes. Usually, such a modular structure is uncovered by the optimisation of some quality function. Although flawed, Modularity remains one of the most popular quality functions. The Louvain algorithm was originally developed for optimising Modularity, but has been applied to a variety of methods. As such, speeding up the Louvain algorithm, enables the analysis of larger graphs in a shorter time for various methods. We here suggest to consider moving nodes to the community of a random neighbour, instead of the best neighbouring community. Although incredibly simple, it reduces the theoretical runtime complexity from \$O(m)\$ to \$O(n \ensuremath{\backslash}log \ensuremath{<}k\ensuremath{>})\$ in networks with a clear community structure. In benchmark networks, resembling real networks more closely, it speeds up the algorithm roughly 2-3 times. This is due to two factors: (1) a random neighbour is likely to be in a "good" community; and (2) random neighbours are likely to be hubs, helping the convergence. Finally, the performance gain only slightly diminishes the quality, thus providing an excellent quality-performance ratio. However, these gains do not seem to hold up when detecting small communities in large graphs.},
  timestamp = {2015-05-27 22:09:43},
  author = {Traag, V. A.},
  urldate = {2015-05-27},
  date = {2015-03-04},
  keywords = {Computer Science - Data Structures and Algorithms,Computer Science - Social and Information Networks,Physics - Physics and Society},
  file = {arXiv.org Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/3GF2KUAI/1503.html:text/html;Traag_2015.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/DPCTF6MH/Traag_2015.pdf:application/pdf},
  eprinttype = {arxiv},
  eprint = {1503.01322}
}

@article{Triulzi2014,
  title = {R\&D and knowledge dynamics in university-industry relationships in biotech and pharmaceuticals: an agent-based model},
  volume = {13},
