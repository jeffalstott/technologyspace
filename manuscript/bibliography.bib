  pages = {031006--031006},
  file = {Fu_et_al_2013.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/3DDTW35T/Fu_et_al_2013.pdf:application/pdf}
}

