  file = {IEEE Xplore Abstract Record:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/B525QDSR/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/53ZS6SGX/Fernandez and Williams - 2010 - Closed-Form Expression for the Poisson-Binomial Pr.pdf:application/pdf}
