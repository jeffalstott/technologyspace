  keywords = {prediction markets,replications,reproducibility},
  file = {PNAS-2015-Dreber-15343-7.pdf:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/66V96XHK/PNAS-2015-Dreber-15343-7.pdf:application/pdf;Snapshot:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/AWBDCG49/15343.html:;Full Text PDF:/Users/jeff/Library/Application Support/Zotero/Profiles/eaysq4eo.default/zotero/storage/CENIWGJP/Dreber et al. - 2015 - Using prediction markets to estimate the reproduci.pdf:application/pdf},
  eprinttype = {pmid},
